<?php

require __DIR__ . '/../vendor/autoload.php';

$connectionsPool = new \Apisearch\Server\Socket\ConnectionsPool();
$server = \Apisearch\Server\Socket\ServerFactory::create(
    new \Apisearch\Server\Socket\App(
        $connectionsPool,
        'log'
    ),
    $argv[1],
    'apisearch:logs'
);

$server->run();